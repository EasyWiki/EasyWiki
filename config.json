{
    "Style":
    {
        "title": "EasyWiki",
        "logo": "logo.png",
        "favicon": "favicon.png",
        "theme": "lux-light",
        "maxAge": 864000000
    },
    "Web":
    {
        "port": 443,
        "httpport": 80,
        "cookieSecret": "password",
        "ssl":
        {
            "key": "ssl/ssl.key",
            "cert": "ssl/ssl.crt"
        },
        "disableCache": false,
        "cacheTTL": 120,
        "users": {
            "easy": "wiki"
        },
        "errorPages":
        {
            "404": {
                "title": "Page not found",
                "description": "<i class=\"far fa-frown\"></i> Page not found!",
                "subtitle": "Whoops, this page doesn't exist.",
                "text": ""
            },
            "500": {
                "title": "Error",
                "description": "An error has occured!",
                "subtitle": "If this error keeps happening, contact an admin.",
                "text": ""
            }
        },
        "cookieNotice":
        {
            "title": "This website uses cookies",
            "message": "We use cookies to improve your experience. We use them to collect analytical data and to save your site preferences.",
            "accept": "Accept",
            "readmore": "Read More",
            "link": "/privacy"
        },
        "analytics": ""
    },
    "Gitter":
    {
        "repo": "https://github.com/WatcherWhale/EasyWiki-Testrepo.git",
        "timeout": 600000
    },
    "Logger":
    {
        "max": 10000
    }
}